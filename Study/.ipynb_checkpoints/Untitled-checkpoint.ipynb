   "source": [
    "# K번째 약수\n",
    "# N의 약수 중 K번 째로 작은 수\n",
    "\n",
    "def solution(N, K):\n",
    "    arr = []\n",
    "    for i in range(1, N+1):\n",
    "        if N % i == 0:\n",
    "            arr.append(i)\n",
    "    arr.sort()\n",
    "    return arr[K-1]\n",
    "    "
   ]
  },
